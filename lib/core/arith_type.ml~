type op =
  | Add
  | Sub
  | Mult
  | Div
  | Mod [@@deriving eq,ord,show,iter,map,fold,sexp]

type 'var gen_t =
  | Int of int
  | Var of 'var
  | Op  of op * 'var gen_t list [@@deriving eq,ord,show,iter,map,fold,sexp]

type t = [`Int] Id.t gen_t [@@deriving eq,ord,show,iter,map,fold,sexp]

           
  
